generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Feedback {
  id         String   @id @default(cuid())
  name       String
  email      String
  profession String?
  message    String
  created_at DateTime @default(now())
}

model Project {
  id          String   @id @default(cuid())
  title       String
  description String
  tech        String[]
  live        String?
  repo        String?
  created_at  DateTime @default(now())
}
